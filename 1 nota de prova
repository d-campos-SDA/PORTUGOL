programa
{
	
	funcao inicio()
	{
		inteiro numero = 8.5
		cadeia nome 
		nome = "julia"

		escreva("informe seu nome: ")
		leia(nome)
		
		escreva ("a nota da prova foi: ", numero, "\n")
	}
}
